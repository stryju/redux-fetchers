    "@types/redux": "^3.6.0",
    "babel-preset-es2015": "^6.22.0",
    "jest": "^19.0.2",
    "react": "15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "5.0.2",
    "redux": "3.6.0",
    "ts-jest": "^19.0.0",
    "typescript": "2.1.6",
    "webpack": "^2.2.1",
    "awesome-typescript-loader": "^3.1.2",
    "html-webpack-plugin": "^2.28.0"
  },
  "peerDependencies": {
    "react-redux": "*",
    "react": "*"
  },
