{
  "name": "redux-fetchers",
  "version": "0.1.0",
  "description": "Component wrapper to automatically dispatch actions when needed",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "build-example": "rm -rf ./example-dist && webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LIQIDTechnology/redux-fetchers.git"
  },
  "keywords": [
    "redux",
    "fetch",
    "network",
    "utility",
    "dispatch"
  ],
  "author": "Tom Nick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LIQIDTechnology/redux-fetchers/issues"
  },
  "homepage": "https://github.com/LIQIDTechnology/redux-fetchers#readme",
  "jest": {
    "globals": {
      "__TS_CONFIG__": {
        "module": "commonjs",
        "jsx": "react",
        "noEmitHelpers": false
      }
